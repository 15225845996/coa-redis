{
  "name": "coa-redis",
  "version": "1.2.1-0",
  "description": "redis component for coa",
  "keywords": [
    "coajs",
    "coa",
    "redis"
  ],
  "license": "MIT",
  "author": "Aex",
  "homepage": "https://github.com/coajs/coa-redis",
  "repository": {
    "type": "git",
    "url": "https://github.com/coajs/coa-redis.git"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc && cp package.json dist",
    "npm-prerelease": "yarn build && yarn version --prerelease && cp package.json README.md dist && cd dist && npm publish",
    "npm-publish": "yarn build && yarn version --patch && cp package.json README.md dist && cd dist && npm publish"
  },
  "dependencies": {
    "@types/ioredis": "^4.17.6",
    "ioredis": "^4.19.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "coa-echo": "^1.0.8",
    "coa-error": "^1.0.4",
    "coa-helper": "^1.0.5",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "coa-echo": "1",
    "coa-error": "1",
    "coa-helper": "1"
  }
}
